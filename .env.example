#panic|fatal|error|warn|info|debug|trace default=info
DEBUG_LEVEL=info

CLIENT_PORT=8090
CLIENT_GRPC_PORT=8091
SERVER_PORT=8081
REQUEST_TIMEOUT_IN_MILLISECOND=700

DB_HOST=localhost
#for test use name from QA_DB_PORT(5433)

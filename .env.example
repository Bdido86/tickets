DB_NAME=movie_tickets

GOOSE_DRIVER=postgres
GOOSE_DBSTRING="user=${DB_USER} password=${DB_PASSWORD} dbname=${DB_NAME} sslmode=disable"
GOOSE_MIGRATION_DIR=./migrations


# QA Settings
QA_DB_HOST=localhost
